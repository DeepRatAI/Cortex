        }
      ]
    },
    {
      "type": "graph",
      "title": "Retrieved chunks",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(cka_retrieved_chunks_count_bucket[5m])) by (le))"
        }
      ]
    }
  ],
